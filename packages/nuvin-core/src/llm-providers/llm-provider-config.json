{
  "providers": [
    {
      "key": "deepinfra",
      "type": "openai-compat",
      "baseUrl": "https://api.deepinfra.com/v1/openai",
      "defaultModels": [
        "meta-llama/Meta-Llama-3.1-70B-Instruct",
        "meta-llama/Meta-Llama-3.1-8B-Instruct"
      ],
      "features": {
        "promptCaching": false,
        "getModels": true
      }
    },
    {
      "key": "openrouter",
      "type": "openai-compat",
      "baseUrl": "https://openrouter.ai/api/v1",
      "defaultModels": [
        "openai/gpt-4o",
        "openai/gpt-4o-mini"
      ],
      "features": {
        "promptCaching": true,
        "getModels": true,
        "includeUsage": true
      }
    },
    {
      "key": "zai",
      "type": "openai-compat",
      "baseUrl": "https://api.z.ai/api/coding/paas/v4",
      "defaultModels": [
        "glm-4.7",
        "glm-4.6",
        "glm-4.5"
      ],
      "features": {
        "promptCaching": false,
        "getModels": false
      }
    },
    {
      "key": "zai-anthropic",
      "type": "anthropic-compat",
      "baseUrl": "https://api.z.ai/api/anthropic/v1",
      "defaultModels": [
        "glm-4.7",
        "glm-4.6",
        "glm-4.5"
      ],
      "features": {
        "promptCaching": false,
        "getModels": false
      }
    },
    {
      "key": "moonshot",
      "type": "openai-compat",
      "baseUrl": "https://api.moonshot.ai/v1",
      "defaultModels": [
        "kimi-latest",
        "kimi-k2-thinking"
      ],
      "features": {
        "promptCaching": false,
        "getModels": true
      }
    },
    {
      "key": "kimi",
      "label": "Kimi for Coding",
      "type": "openai-compat",
      "baseUrl": "https://api.kimi.com/coding/v1",
      "features": {
        "promptCaching": true,
        "getModels": true,
        "includeUsage": true
      },
      "customHeaders": {
        "User-Agent": "KimiCLI/0.59"
      }
    },
    {
      "key": "minimax",
      "label": "MiniMax Coding Plan",
      "type": "anthropic-compat",
      "baseUrl": "https://api.minimax.io/anthropic/v1",
      "models": [
        {
          "id": "MiniMax-M2.1",
          "name": "MiniMax M2.1",
          "limits": {
            "contextWindow": 200000,
            "maxOutput": 16000
          }
        }
      ],
      "defaultModels": [
        "MiniMax-M2.1"
      ],
      "features": {
        "promptCaching": false,
        "getModels": true,
        "includeUsage": false
      }
    }
  ]
}